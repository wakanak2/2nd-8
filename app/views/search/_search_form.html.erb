<%= form_tag(search_path, :method =>'get') do %>
   <%= select_tag(:option,options_for_select([["user" ,"1"],["book","2"]]))%>
   <%= select_tag(:choice,options_for_select([["完全一致","1"],["前方一致","2"],["後方一致","3"],["部分一致","4"]]))%>
   <%= text_field_tag :search %>
   <%= submit_tag 'Search',:name => nil %>
<% end %>